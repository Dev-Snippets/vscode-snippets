{
  "name": "dd-vscode-snippets",
  "version": "0.0.5",
  "displayName": "VSCode Snippets & Tasks for Docs and Development",
  "description": "A collection of essentials snippets for Visual Studio Code.",
  "publisher": "wx-chevalier",
  "icon": "images/logo.png",
  "galleryBanner": {
    "color": "#333333",
    "theme": "dark"
  },
  "homepage": "https://github.com/wx-chevalier/@m-fe/vscode-snippets/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-chevalier/@m-fe/vscode-snippets.git"
  },
  "bugs": {
    "url": "https://github.com/wx-chevalier/@m-fe/vscode-snippets/issues"
  },
  "categories": [
    "Snippets",
    "Formatters"
  ],
  "keywords": [
    "Snippet",
    "Task"
  ],
  "engines": {
    "vscode": "^1.50.0"
  },
  "scripts": {
    "build": "vsce package",
    "publish": "vsce publish"
  },
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "snippets/angular-directive.json"
      },
      {
        "language": "shellscript",
        "path": "snippets/bash.json"
      },
      {
        "language": "go",
        "path": "snippets/go.json"
      },
      {
        "language": "json",
        "path": "snippets/json.json"
      },
      {
        "language": "markdown",
        "path": "snippets/markdown.json"
      },
      {
        "language": "plaintext",
        "path": "snippets/plaintext.json"
      },
      {
        "language": "properties",
        "path": "snippets/properties.json"
      },
      {
        "language": "typescript",
        "path": "snippets/react-ts.json"
      },
      {
        "language": "shellscript",
        "path": "snippets/shellscript.json"
      },
      {
        "language": "yaml",
        "path": "snippets/yaml.json"
      }
    ],
    "taskDefinitions": [
      {
        "label": "Run google java format",
        "type": "shell",
        "command": "google-java-format --replace ${file}"
      }
    ]
  },
  "devDependencies": {
    "vsce": "1.81.1",
    "@types/node": "^14.11.10",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "@types/vscode": "^1.50.0"
  },
  "license": "SEE LICENSE IN LICENSE"
}
